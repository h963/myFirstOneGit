/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clearFix::after,

.clearFix::before{
	content: '';
	font-size: 0;
	height: 0;
	line-height: 0%;
	display: block
}
.clearFix::after{
	clear: both;
}